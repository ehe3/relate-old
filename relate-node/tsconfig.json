{
  "compilerOptions": {
      "resolveJsonModule": true,
      "target": "es6",
      "module": "commonjs",
      "allowJs": true,
      "outDir": "./dist",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "useUnknownInCatchVariables": false
    }
}
